
<% content_for :title, "Elements" %>

<%= link_to "Nuevo elemento", new_element_path, class: "btn btn-primary float-end mx-1" %>
<%= link_to "Volver", statics_index_path, class: "btn btn-secondary float-end" %>
<h1>Elementos</h1>

<div id="elements">
  <% @elements.each do |element| %>
    <%= render element %>
      <%= link_to "Modificar", edit_element_path(element) %><br>
  <% end %>
</div>
